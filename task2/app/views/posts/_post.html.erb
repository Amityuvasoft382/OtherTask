<% @posts.each do |post|%>

<tr>
	<td></td>
<td align="center"><%= post.title%></td>
<td align="center"><%= User.find(post.user_id).email %></td>

<td align="center"><%= link_to "Show", post %></td>
<% if user_signed_in? %>
<% if current_user.id == post.user_id%>
<td align="center"><%= link_to "Edit", edit_post_path(post) %></td>
<td align="center"><%= link_to "delete", post, method: :delete,remote:true,data: { confirm: 'Are you sure?' }%></td>
<%end%>
<%end%>
</tr>

<%end%>