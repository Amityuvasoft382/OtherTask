<% @products.each do |product|%>

<tr align="center">
<td align="center"><%= product.c_name%></td>
<td align="center"><%= product.p_name %></td>
<td align="center"><%= link_to "Show", product %></td>

<td align="center"><%= link_to "Edit", edit_product_path(product) %></td>
<td align="center"><%= link_to "delete", product, method: :delete,remote:true,data: { confirm: 'Are you sure?' }%></td>
</tr>

<%end%>